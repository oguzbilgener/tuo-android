<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/navigation_drawer_outer_layout"
    tools:context="co.uberdev.ultimateorganizer.android.ui.HomeNavigationDrawerFragment">

    <LinearLayout
        android:layout_width="210dp"
        android:layout_height="100dp"></LinearLayout>

    <LinearLayout
        android:background="#000000"
        android:layout_width="210dp"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <LinearLayout
            android:background="@color/title_section_overview"
            android:layout_width="10dp"
            android:layout_height="50dp">

        </LinearLayout>

        <LinearLayout
            android:background="#ffffff"
            android:layout_width="200dp"
            android:layout_height="50dp">

            <TextView
                style="@style/navigation_drawer_element"
                android:text="Overview"/>


        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:background="#000000"
        android:layout_width="210dp"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <LinearLayout
            android:background="@color/title_section_calendar"
            android:layout_width="10dp"
            android:layout_height="50dp">

        </LinearLayout>

        <LinearLayout
            android:background="#ffffff"
            android:layout_width="200dp"
            android:layout_height="50dp">

            <TextView
                style="@style/navigation_drawer_element"
                android:text="Calendar"/>


        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:background="#000000"
        android:layout_width="210dp"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <LinearLayout
            android:background="@color/title_section_notes"
            android:layout_width="10dp"
            android:layout_height="50dp">

        </LinearLayout>

        <LinearLayout
            android:background="#ffffff"
            android:layout_width="200dp"
            android:layout_height="50dp">

            <TextView
                style="@style/navigation_drawer_element"
                android:text="Notes"/>


        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:background="#000000"
        android:layout_width="210dp"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <LinearLayout
            android:background="@color/title_section_academic_schedule"
            android:layout_width="10dp"
            android:layout_height="50dp">

        </LinearLayout>

        <LinearLayout
            android:background="@color/button_dark_text"
            android:layout_width="200dp"
            android:layout_height="50dp">

            <TextView
                style="@style/navigation_drawer_element"
                android:text="Schedule"/>


        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:background="#000000"
        android:layout_width="210dp"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <LinearLayout
            android:background="@color/title_section_academic_network"
            android:layout_width="10dp"
            android:layout_height="50dp">

        </LinearLayout>

        <LinearLayout
            android:background="#ffffff"
            android:layout_width="200dp"
            android:layout_height="50dp">

            <TextView
                style="@style/navigation_drawer_element"
                android:text="Network"/>


        </LinearLayout>

    </LinearLayout>



</LinearLayout>

